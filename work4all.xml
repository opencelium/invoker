    <name>work4all</name>
    <description>work4all description</description>
    <hint>This interface provides a token auth. Read the documentation here https://demo.work4allcloud.de:4713/swagger/index.html</hint>
    <requiredData>
        <item name="url" type="string" visibility="public"/>
        <item name="username" type="string" visibility="public"/>
        <item name="password" type="string" visibility="public"/>
        <item name="token" type="string" visibility="private">%{APIAuth.body.Token}</item>
    </requiredData>
    <authType>token</authType>
    <category_tags>
