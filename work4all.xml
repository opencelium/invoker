    <operations>
        <operation name="APIAuth" type="test">
            <request>
                <method>POST</method>
                <endpoint>{url}/api/Auth/login</endpoint>
                <header>
                    <item name="X-Version" type="string">2</item>
                </header>
                <body type="object" format="json" data="raw">
                    <field name="username" type="string">{username}</field>
                    <field name="passwordHash" type="string">{password}</field>
                    <field name="application" type="string"/>
                    <field name="refreshToken" type="string"/>
                    <field name="deviceType" type="string"/>
                </body>
            </request>
            <response>
                <success status="200">
                    <header/>
                    <body type="object" format="json" data="raw">
                        <field name="token" type="string"/>
                        <field name="refreshToken" type="string"/>
                        <field name="benutzerCode" type="string"/>
                        <field name="benutzerRechte" type="string"/>
                        <field name="kundennummer" type="string"/>
                        <field name="module" type="string"/>
                        <field name="needsPasswordChange" type="string"/>                        
                    </body>
                </success>
                <fail status="401">
                    <header/>
                    <body/>
                </fail>
	</response>
	</operation>	
