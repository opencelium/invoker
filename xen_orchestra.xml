        <operation name="GetVersion" type="test">
            <request>
                <method>GET</method>
                <endpoint>{url}/rest/v0/</endpoint>
                <header>
                    <item name="Cookie" type="string">authenticationToken={authenticationToken}</item>
                    <item name="Content-Type" type="string">application/json</item>
                </header>
                <body type="object" format="json" data="raw">
                </body>
            </request>
            <response>
                <success status="200">
                    <header/>
                    <body type="object" format="json" data="raw">
                        <field name="token" type="string"/>
                        <field name="refreshToken" type="string"/>
                        <field name="benutzerCode" type="string"/>
                        <field name="benutzerRechte" type="string"/>
                        <field name="kundennummer" type="string"/>
                        <field name="module" type="string"/>
                        <field name="needsPasswordChange" type="string"/>                        
                    </body>
                </success>
                <fail status="401">
                    <header/>
                    <body/>
                </fail>
            </response>
        </operation>                     
